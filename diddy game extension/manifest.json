{
  "manifest_version": 3,
  "name": "Diddy Game",
  "version": "1.2",
  "description": "Diddy Game as a Chrome/Edge extension opens in a new tab.",
  "icons": {
    "48": "icon-48.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open Diddy Game"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}