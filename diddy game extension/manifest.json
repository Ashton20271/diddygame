{
  "manifest_version": 3,
  "name": "Diddy Game",
  "version": "1.0.0",
  "description": "Play Diddy Game in a new tab.",
   "icons": {
    "48": "icon-48.png"
  },
  "permissions": [],
  "action": {
    "default_title": "Play Diddy Game"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "background": {
    "service_worker": "background.js"
  }
}